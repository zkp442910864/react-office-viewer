{
  "name": "@zzzz-/react-utils",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "webpack serve --env CUSTOM_NODE_ENV=development --mode=development --config config/index.js",
    "start:no-mock": "webpack serve --env CUSTOM_NODE_ENV=development mock=no --mode=development --config config/index.js",
    "build": "webpack build --env CUSTOM_NODE_ENV=production --mode=production --config config/index.js",
    "clear": "gulp run",
    "build:umd": "webpack build --env CUSTOM_NODE_ENV=production lib=umd --mode=production --config config/index.js",
    "build:es": "babel src --out-dir ./dist/es --extensions .tsx --extensions .ts --ignore src/**/*.stories.tsx --ignore src/views --ignore src/types --ignore src/stories --ignore src/router --ignore src/App.tsx --ignore src/main.ts --source-maps --copy-files  --no-copy-ignored",
    "build:tsc-types": "tsc -p tsconfig.build.json",
    "build:lib": "npm run clear && npm run build:tsc-types && npm run build:es && npm run build:umd",
    "publish:patch": "npm run build:lib && npm version patch && npm publish",
    "copy:less": "copyfiles -u 1 src/**/*.scoped.less lib/",
    "copy:types": "copyfiles -u 1 src/types/**/* types/",
    "jest": "jest --passWithNoTests"
  },
  "main": "dist/index.js",
  "module": "es/tsc.js",
  "types": "types/tsc.d.ts",
  "peerDependencies": {
    "react": ">=16.0.0 || >=17.0.0 || >=18.0.0",
    "react-dom": ">=16.0.0 || >=17.0.0 || >=18.0.0"
  },
  "dependencies": {
